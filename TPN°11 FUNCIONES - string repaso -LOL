#include <bits/stdc++.h>
using namespace std;

void cargarMatriz(int F, int C);

int main(){
    int F = 0;
    int C = 0;
    cin >> F;
    cin >> C;
    cargarMatriz(F, C);
}

void cargarMatriz(int F, int C){
    vector<vector<char> > Matriz(F, vector<char>(C));
    char Letra;
    for(int i = 0; i < F; i++){
        if(i % 2 == 0){
            Letra = 'L';
        }
        if(i % 2 != 0){
            Letra = 'O';
        }
        for(int j = 0; j < C; j++){
            Matriz[i][j] = Letra;
            cout<<Letra;
            
        }
        cout<<endl;
    }   
}
